<?xml version="1.0" encoding="utf-8"?>

<!-- Slam 配置文件 -->
<Slam Sendfreq="30" Display="0" Showfreq="100" MinGpsAccu="50">
  <!-- 0:autodecide, 1:usingcompare, 2:usingGPSandHeading, 3:usingslampos, 4:usingreckon, 5:usingvelocity -->
  <Method var="3"/>  
  <Obstacle 
    movingVx="10" movingVy="010"
    resolutionparam1="1"
    resolutionparam2="1"
    resolutionparam3="1"
    />
  <Data
    length="500000" gpsLength="200000" lidarLen="10"
    xMin="-300" xMax="300" yMin="-300" yMax="800"
    lineOrder ="3" usingRansac="0"
    deltaDisY="-3"
    />
  <Window
    windowX="30"
    windowForward="60"
    windowBackward="-40"
    outputAreaHeight="200"
    windowScale="10"
    />
</Slam>
